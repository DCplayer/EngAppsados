<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.engappsados.engappsadosapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            android:label="@string/title_activity_welcome"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".MenuPrincipalActivity" />
        <activity android:name=".Rewards" />
        <activity android:name=".TareasActivity" />
        <activity android:name=".Noticias" />
        <activity android:name=".politicaActivity" />
        <activity android:name=".userProfile" />
        <activity
            android:name=".MenuTabbed"
            android:label="@string/title_activity_menu_tabbed"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>